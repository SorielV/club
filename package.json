{
  "name": "foo",
  "version": "1.0.0",
  "description": "Clubes",
  "author": "SorielV",
  "private": true,
  "scripts": {
    "dev": "cross-env client-env=true NODE_ENV=development nodemon --exec babel-node --presets @babel/preset-env server/index.js --watch server",
    "seo-dev": "nuxt generate & cross-env NODE_ENV=development nodemon --exec babel-node --presets @babel/preset-env server/index.js --watch server",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.0.0",
    "@toast-ui/vue-calendar": "^1.0.3",
    "bcrypt": "^3.0.3",
