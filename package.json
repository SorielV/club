    "cross-env": "^5.2.0",
    "express": "^4.16.3",
    "faker": "^4.1.0",
    "helmet": "^3.15.0",
    "husky": "^1.3.1",
    "jsonwebtoken": "^8.4.0",
    "knex": "^0.16.3",
    "lint-staged": "^8.1.3",
    "marked": "^0.6.0",
    "module-alias": "^2.1.0",
    "morgan": "^1.9.1",
    "nuxt": "^2.0.0",
    "nuxt-buefy": "^0.2.1",
    "pg": "^7.7.1",
